<div id="ar">
  <a-scene vr-mode-ui='enabled: false' arjs='sourceType: webcam; videoTexture: true; debugUIEnabled: false' renderer='antialias: true; alpha: true'>
    <a-camera gps-camera rotation-reader></a-camera>
    <a-entity gltf-model="<%= asset_path "airplane/scene.gltf" %>" position="<%= "0 #{@flight.altitude} 0 "%>" scale="10 10 10" gps-entity-place="<%= "latitude: #{@flight.latitude}; longitude:#{@flight.longitude}" %>"/>
  </a-scene>
</div>
      <% if false %>
        <div class="container">
      <h1>Play!</h1>
      <%= simple_form_for @game do |f| %>
        <%= f.input :arrival_airport_guess_id, collection: @airports %>
        <%= f.input :departure_airport_guess_id, collection:  @airports %>
        <%= f.input :airline_guess_id, collection: @airline %>
        <%= f.submit 'Catch this flight!', class: 'btn mt-2'%>
      <%end%>
    </div>
  <% end %>
